		"husky": "^8.0.1",
		"nodemon": "^2.0.19",
		"prettier": "^2.8.1",
		"pretty-quick": "^3.1.3",
		"rollup": "^2.78.1",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-import-css": "^3.1.0",
		"rollup-plugin-terser": "^7.0.2"
	},
	"dependencies": {
