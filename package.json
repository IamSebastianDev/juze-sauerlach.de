	"private": true,
	"scripts": {
		"preci": "rm -rf node_modules",
		"ci": "yarn install --frozen-lockfile",
		"postinstall": "husky install",
		"db:up": "docker-compose up -d",
		"db:down": "docker-compose down -d",
		"dev": "rollup --config .config/rollup.config.js --watch",
		"build": "rollup --config .config/rollup.config.js",
		"serve": "yarn db:up && nodemon server/server.mjs --config ./config/nodemon.config.json",
		"start": "yarn db:up && node server/server.mjs"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.1",
		"@commitlint/config-conventional": "^17.1.0",
		"@rollup/plugin-commonjs": "^22.0.2",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"husky": "^8.0.1",
		"nodemon": "^2.0.19",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"rollup": "^2.78.1",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-terser": "^7.0.2"
	},
	"dependencies": {
		"@editorjs/editorjs": "^2.25.0",
		"@editorjs/paragraph": "^2.8.0",
		"bcrypt": "^5.0.1",
		"compression": "^1.7.4",
		"connect-mongodb-session": "^3.1.1",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-fileupload": "^1.4.0",
		"express-session": "^1.17.3",
		"helmet": "^6.0.0",
		"mongodb": "^4.9.0",
		"nodemailer": "^6.7.8",
		"pangolicons": "^3.2.1",
		"passport": "^0.6.0",
		"passport-local": "^1.0.0"
	}
}
